/* /点赞 */
.animated {
	animation-duration: 1s;
}

.aeet {
	position: absolute;
	width: 45rpx;
	height: 60rpx;
}


@keyframes fadeIn {
	0% {
		opacity: 1;
		transform-origin: 50% 50%;
		transform: scale(1, 1);
		-webkit-filter: blur(0px);
	}

	100% {
		transform-origin: 50% 50%;
		transform: scale(1.5, 1.5);
		-webkit-filter: blur(2px);
	}
}

.fadeIn {
	animation-name: fadeIn;
}

