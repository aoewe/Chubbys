/* 整屏点赞 */

.anted {
	transform: scale(0.9, 0.9);
}

@keyframes vanishlike {
	0% {
		transform: scale(1, 1);
	}

	25% {
		transform: scale(1.3, 0.7);
	}

	40% {
		transform: scale(0.8, 1.3);
	}

	55% {
		transform: scale(0.9, 1.1);
	}

	75% {
		transform: scale(1.1, 0.7);
	}

	100% {
		transform: scale(1, 1);
	}
}

.vanishlike {
	animation-name: vanishlike;
}

