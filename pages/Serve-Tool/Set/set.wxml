      <navigator class="cont-list" url="../upPayPaw/upPayPaw">
        <view class="cont-txt">密码修改</view>
        <view class="cont-left">
          <van-icon name="arrow" />
        </view>
			</navigator>
			
			<view class="btn">
		<van-button custom-class="external-class" color="linear-gradient(0deg, #EC6601, #FFDC03);" bindtap="exit" size="round" round>退出登录</van-button>
	</view>
