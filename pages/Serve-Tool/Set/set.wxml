<view>
	<view class="logo">
		<image src="../../../static/imgs/logo.png"></image>
	</view>
	<view class="bdd">棒多多</view>

  <view class="content">
		<navigator class="cont-list">
      <view class="cont-txt">版本号</view>
      <view class="cont-left">
        <view style="font-size: 26rpx;">1.0.1</view>
        <van-icon name="arrow" />
      </view>
    </navigator>
    <navigator class="cont-list" url="/pages/Serve-Tool/Headimg/Headimg">
      <view class="cont-txt">信息修改</view>
      <view class="cont-left">
        <image binderror="imgError" style="width: 64rpx;height: 64rpx;border-radius: 50%;" src="{{userInfo.avatar}}" />
        <van-icon name="arrow" />
      </view>
    </navigator>
    <navigator class="cont-list" url="/pages/Address/GetAddress/getAddress">
      <view class="cont-txt">收货地址</view>
      <view class="cont-left">
        <van-icon name="arrow" />
