page {
  background-color: #ffffff;
}

.list {
  padding: 30rpx 0 30rpx 30rpx;
}

.item {
  display: flex;
  align-items: center;
  height: 150rpx;
  position: relative;
}

.item::after {
  content: '';
  position: absolute;
  right: 0;
  bottom: 0;
  background-color: #EEEEEE;
  width: 607rpx;
  height: 2rpx;
}

.item>view {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding-bottom: 6rpx;
  margin-left: 14rpx;
  width: 100%;
  height: 102rpx;
}

.item>view text:last-child {
  color: #999999;
  font-size: 28rpx;
}

.head-flex {
	display: flex;
	align-items: center;
}

.head-txt {
	margin: 0rpx 21rpx;
	margin-left: 5rpx;
	font-size: 34rpx;
	font-family: PingFang SC;
	font-weight: 500;
	color: #1D1D1D;
}

.head-grad-icon {
	width: 36rpx;
	height: 40rpx;
}

.head-grad-text {
	position: relative;
	right: 8rpx;
	z-index: -1;
	font-size: 24rpx;
	font-family: PingFang SC;
	font-weight: bold;
	color: #FFFFFF;
	padding: 1rpx 15rpx;
	height: 40rpx;
	background: #18559A;
	border-radius: 0rpx 20rpx 20rpx 0rpx;
}