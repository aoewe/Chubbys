.item .status {
	padding: 0 16rpx 24rpx;
	font-size: 28rpx;
	display: flex;
	justify-content: flex-end;
	align-items: center;
}

.item .status button {
	width: 168rpx;
	height: 68rpx;
	font-size: 26rpx;
	padding: 0;
	font-weight: 400;
}

.status-t {
	color: #FF4701;
	font-size: 28rpx;
}

.invalid-status-t {
	color: #999999;
}

.status {
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-top: 20rpx;
}

.status-b {
	margin-left: 32rpx;

}

.status-b .van-button {
	height: 58rpx;
	padding: 0 16rpx;
	border-radius: 34rpx;
	margin-left: 10rpx;
	z-index: 9;
}

.white_status_bottom {
	width: 168rpx;
	height: 68rpx;
	border: 1px solid #666666;
	border-radius: 34rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 28rpx;
	font-family: PingFang SC;
	font-weight: 400;
	color: #666666;
}

.status_bottom {
	width: 168rpx;
	height: 68rpx;
	border: 1px solid #FF4701;
	border-radius: 34rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 28rpx;
	font-family: PingFang SC;
	font-weight: 400;
	color: #FF4701;
}