page {
  background: #FE6231;
}

.rush_white {
  margin: auto;
  margin-top: 70rpx;
  width: 702rpx;
  height: 1252rpx;
  background: #FFFFFF;
  border-radius: 10rpx 10rpx 12rpx 12rpx;
}

.rob_red {
  width: 228rpx;
  height: 84rpx;
}

.rush_up {
  display: flex;
  align-items: center;
}

.Trans_block {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #ED213A;
}

.item {
  display: inline-block;
  width: 54rpx;
  height: 34rpx;
  background: #FF4B4E;
  border-radius: 17rpx;
  margin-right: 5rpx;
  color: #fff;
  font-size: 12px;
  text-align: center;
  padding: 2rpx;
}

.com_img {
  width: 232rpx;
  height: 232rpx;
  background: #999999;
  border-radius: 8rpx;
  margin: 18rpx;
}

.ia-text {
  margin: auto;
  width: 410rpx;
  margin-top: 20rpx;
  margin-bottom: 70rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #000000;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}


.i-blocka {
  margin: auto;
  height: 256rpx;
  border-radius: 20rpx;
  display: flex;
}

.Limit {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #F3280D;
  margin-bottom: 20rpx;
}

.rob_yellow {
  position: absolute;

  width: 383rpx;
  height: 62rpx;
}

.rob_money {
  position: relative;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
  top: 6rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FF0000;
}

.rob_money text {
  font-size: 40rpx;
  font-family: Heiti SC;
  font-weight: 500;
  color: #FF0000;
}

.rob_background {
  position: absolute;
  bottom: 0;
  z-index: -1;
  width: 100%;
  height: 271rpx;
}


.time {
  color: #FF2A48;
  text-align: center;
  display: flex;
  font-size: 24rpx;
}

.time>view {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #FF2A48;
  color: #FFFFFF;
  border-radius: 4rpx;
  width: 40rpx;
  height: 30rpx;
  padding: 5rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 34rpx;
  margin: 0 4rpx;
}